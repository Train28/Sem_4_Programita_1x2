@model Sem_4_Programita_1x2.Models.Factura
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
    <title>Factura</title>
</head>
<body>
    <div class="container">
        <label><b>Invoice </b></label>
        <div class="form-group">@Html.DisplayTextFor(model=>model.invoice)</div><br/>
        <label><b>Cliente </b></label>
        <div class="form-group">@Html.DisplayTextFor(model=>model.cliente)</div><br/>
        <label><b>Subtotal </b></label>
        <div class="form-group">@Html.DisplayTextFor(model=>model.subtotal)</div><br/>
        <label><b>Total </b></label>
        <div class="form-group">@Html.DisplayTextFor(model=>model.total)</div><br/>
        <div class="form-group">@Html.DisplayTextFor(model=>model.fecha)</div><br/>
    </div>
</body>
</html>